            identifier,
            ...context.data,
        },
    }, language);
}
/**
 * Lança erro de permissões insuficientes
 */
function throwInsufficientPermissions(requiredPermission, context = {}, language = 'pt-BR') {
    throw new AppError_1.AppError('USUARIO_INSUFFICIENT_PERMISSIONS', {
        ...context,
        data: {
            requiredPermission,
            ...context.data,
        },
    }, language);
}
