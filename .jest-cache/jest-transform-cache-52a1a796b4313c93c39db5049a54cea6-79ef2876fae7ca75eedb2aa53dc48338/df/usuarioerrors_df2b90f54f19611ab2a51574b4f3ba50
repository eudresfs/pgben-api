 * Lança erro de senha fraca
 */
function throwWeakPassword(context = {}, language = 'pt-BR') {
    throw new AppError_1.AppError('USUARIO_PASSWORD_WEAK', {
        ...context,
        data: {
            field: 'password',
            ...context.data,
        },
    }, language);
}
