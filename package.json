{
  "name": "pgben-server",
  "version": "0.1.12",
  "private": true,
  "license": "UNLICENSED",
  "description": "Plataforma de gestão de benefícios eventuais do Governo Federal do Brasil destinado à secretarias municipais de assistência social",
  "author": "Eudres Freitas",
  "engines": {
    "node": ">=18 <23",
    "npm": ">=10.8.2"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "check": "npx tsc --noEmit",
    "build": "nest build",
    "release": "npx standard-version && git push --follow-tags origin main",
    "prestart:prod": "npm run build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "npx ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:pm2": "pm2 start dist/main.js --no-daemon --name pgben-server",
    "start:prod": "node dist/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create ./src/database/migrations/%npm_config_name%",
    "migration:generate": "npm run typeorm -- migration:generate ./src/database/migrations/%npm_config_name%",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show",
    "seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/seeds/seed-source.ts",
    "seed:run": "npm run seed",
    "seed:run:initial": "npm run seed -- -s InitialDataSeed",
    "seed:run:test": "npm run seed -- -s TestDataSeed",
    "seed:execute": "ts-node -r tsconfig-paths/register ./src/database/scripts/execute-seeds.ts",
    "seed:diagnose": "ts-node -r tsconfig-paths/register ./src/database/scripts/diagnose-seeds.ts",
    "seed:direct": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seeds.ts",
    "seed:permissions": "psql -U %npm_config_user% -d %npm_config_db% -h %npm_config_host% -f ./src/database/scripts/permission-seed.sql",
    "db:drop": "npm run typeorm -- schema:drop",
    "db:setup": "npm run migration:run && npm run seed:direct",
    "db:reset": "npm run typeorm -- schema:drop && npm run db:setup",
    "db:setup:test": "npm run migration:run && npm run seed:run:initial && npm run seed:run:test",
    "db:fix:generate": "ts-node scripts/database/fix-migrations.ts generate",
    "db:fix:run": "ts-node scripts/database/fix-migrations.ts run",
    "db:fix:revert": "ts-node scripts/database/fix-migrations.ts revert",
    "db:fix:status": "ts-node scripts/database/fix-migrations.ts status",
    "db:backup": "ts-node scripts/database/create-backup.ts",
    "db:restore": "ts-node scripts/database/restore-backup.ts",
    "db:check": "ts-node scripts/database/check-connection.ts",
    "db:dev": "docker-compose -f docker-compose.dev.yml up -d postgres pgadmin",
    "db:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:temp-audit": "jest test/modules/auditoria/services/auditoria.service.spec.ts --coverage --no-watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:auth": "jest --config ./test/jest-e2e.json auth.e2e-spec.ts",
    "test:e2e:beneficio": "jest --config ./test/jest-e2e.json beneficio.e2e-spec.ts",
    "test:integration": "jest --config ./test/jest-e2e.json --testRegex .integration.spec.ts$",
    "test:api": "jest --config ./test/jest-e2e.json --testRegex .api.spec.ts$",
    "test:auth": "jest src/modules/auth/tests",
    "test:cidadao": "jest src/modules/cidadao/tests",
    "test:beneficio": "jest src/modules/beneficio/tests",
    "test:logging": "jest src/shared/logging/tests",
    "test:monitoring": "jest src/shared/monitoring/tests",
    "test:pagamento": "jest --config src/modules/pagamento/tests/jest.config.js",
    "test:pagamento:unit": "jest --config src/modules/pagamento/tests/jest.config.js src/modules/pagamento/tests/unit",
    "test:pagamento:integration": "jest --config src/modules/pagamento/tests/jest.config.js src/modules/pagamento/tests/integration",
    "test:pagamento:e2e": "jest --config src/modules/pagamento/tests/jest.config.js test/pagamento",
    "test:pagamento:coverage": "jest --config src/modules/pagamento/tests/jest.config.js --coverage",
    "docker:dev": "docker-compose --env-file .env up",
    "docker:dev:build": "docker-compose --env-file .env up --build",
    "docker:prod": "docker-compose -f docker-compose.yml --env-file .env.prod up",
    "docker:prod:build": "docker-compose -f docker-compose.yml --env-file .env.prod up --build",
    "docker:stop": "docker-compose --env-file .env down",
    "docker:clean": "docker-compose --env-file .env down -v",
    "docker:logs": "docker-compose --env-file .env logs -f",
    "docker:prod:stop": "docker-compose -f docker-compose.yml --env-file .env.prod down",
    "docker:prod:clean": "docker-compose -f docker-compose.yml --env-file .env.prod down -v",
    "jwt:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-jwt-docker-clean.ps1 -OutputFormat compose",
    "jwt:setup:dev": "powershell -ExecutionPolicy Bypass -File scripts/setup-jwt-docker-clean.ps1 -OutputFormat env",
    "jwt:setup:docker": "powershell -ExecutionPolicy Bypass -File scripts/setup-jwt-docker-clean.ps1 -OutputFormat compose",
    "jwt:setup:k8s": "powershell -ExecutionPolicy Bypass -File scripts/setup-jwt-k8s.ps1",
    "jwt:generate": "node scripts/gerar-chaves-jwt.js",
    "jwt:regenerate": "powershell -ExecutionPolicy Bypass -File scripts/setup-jwt-docker-clean.ps1 -OutputFormat env -Force"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.812.0",
    "@aws-sdk/s3-request-presigner": "^3.812.0",
    "@faker-js/faker": "^9.8.0",
    "@fastify/cors": "^8.5.0",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "4.0.0",
    "@nestjs/bull": "11.0.2",
    "@nestjs/cache-manager": "3.0.1",
