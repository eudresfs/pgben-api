{
  "name": "pgben-server",
  "version": "0.1.1",
  "private": true,
  "license": "UNLICENSED",
  "description": "",
  "author": "",
  "engines": {
    "node": ">=18 <23",
    "npm": ">=8"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "nest build",
    "prestart:prod": "npm run build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:pm2": "pm2 start dist/main.js --no-daemon --name pgben-server",
    "start:prod": "node dist/main",
    "typeorm": "typeorm-ts-node-commonjs -d ./dist/data-source.js",
    "migration:create": "npm run typeorm -- migration:create ./src/database/migrations/%npm_config_name%",
    "migration:generate": "npm run typeorm -- migration:generate ./src/database/migrations/%npm_config_name%",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show",
    "seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/seeds/seed-source.ts",
    "seed:run": "npm run seed",
    "seed:run:initial": "npm run seed -- -s InitialDataSeed",
    "seed:run:test": "npm run seed -- -s TestDataSeed",
    "db:setup": "npm run migration:run && npm run seed:run",
    "db:reset": "npm run typeorm -- schema:drop && npm run db:setup",
    "db:setup:test": "npm run migration:run && npm run seed:run:initial && npm run seed:run:test",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:auth": "jest --config ./test/jest-e2e.json auth.e2e-spec.ts",
    "test:e2e:beneficio": "jest --config ./test/jest-e2e.json beneficio.e2e-spec.ts",
    "test:integration": "jest --config ./test/jest-e2e.json --testRegex .integration.spec.ts$",
    "test:api": "jest --config ./test/jest-e2e.json --testRegex .api.spec.ts$",
    "test:auth": "jest src/modules/auth/tests",
    "test:cidadao": "jest src/modules/cidadao/tests",
    "test:beneficio": "jest src/modules/beneficio/tests",
    "test:logging": "jest src/shared/logging/tests",
    "test:monitoring": "jest src/shared/monitoring/tests",
    "test:pagamento": "jest --config src/modules/pagamento/tests/jest.config.js",
    "test:pagamento:unit": "jest --config src/modules/pagamento/tests/jest.config.js src/modules/pagamento/tests/unit",
    "test:pagamento:integration": "jest --config src/modules/pagamento/tests/jest.config.js src/modules/pagamento/tests/integration",
    "test:pagamento:e2e": "jest --config src/modules/pagamento/tests/jest.config.js test/pagamento",
    "test:pagamento:coverage": "jest --config src/modules/pagamento/tests/jest.config.js --coverage",
    "docker:dev": "docker-compose --env-file .env up",
    "docker:dev:build": "docker-compose --env-file .env up --build",
    "docker:prod": "docker-compose -f docker-compose.yml --env-file .env.prod up",
    "docker:prod:build": "docker-compose -f docker-compose.yml --env-file .env.prod up --build",
    "docker:stop": "docker-compose --env-file .env down",
    "docker:clean": "docker-compose --env-file .env down -v",
    "docker:logs": "docker-compose --env-file .env logs -f",
    "docker:prod:stop": "docker-compose -f docker-compose.yml --env-file .env.prod down",
    "docker:prod:clean": "docker-compose -f docker-compose.yml --env-file .env.prod down -v"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.812.0",
    "@aws-sdk/s3-request-presigner": "^3.812.0",
    "@faker-js/faker": "^9.8.0",
    "@fastify/cors": "^8.5.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/platform-fastify": "^11.1.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/faker": "^6.6.8",
    "@types/handlebars": "^4.0.40",
    "@types/pg": "^8.15.2",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "cache-manager": "^6.4.3",
    "clamscan": "^2.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.0",
    "connect-timeout": "^1.9.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.5.0",
    "exceljs": "^4.4.0",
    "fastify": "^4.29.1",
    "file-type": "^20.5.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "mathjs": "^14.4.0",
    "mime-types": "^3.0.1",
    "minio": "^8.0.5",
    "nest-winston": "^1.10.2",
    "node-cache": "^5.1.2",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.0",
    "pm2": "^6.0.6",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.34.1",
    "typeorm": "^0.3.24",
    "typeorm-extension": "^3.7.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@types/pdfkit": "^0.13.9",
    "@types/sanitize-filename": "^1.1.28",
    "@types/supertest": "^6.0.3",
    "autocannon": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.3",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0"
  }
}
